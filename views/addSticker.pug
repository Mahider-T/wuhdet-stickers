doctype html 

html 

    head
        title = title 
    body 
        form(method = 'POST' action = "/api/orders/addSticker" enctype="multipart/form-data")
            legend Add Sticker

            .form-group 
                label(for='images') Add link
                input.form-control(type='file', name='images', placeholder='Add image', multiple)
                br
            .form-group
                label(for="agentId") Choose agent
                input.form-control(type='text', name='agentId', placeholder='Add agent ID')
                br
            .form-group
                button.btn.btn-primary(type='submit') Add Sticker

